<aura:component controller="GetContactClass" >
    <aura:attribute name="Con" type="Contact"/>
    
    <aura:attribute name="conList" type="Contact[]"/>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <p>List Contacts:</p>
    <aura:iteration items="{!v.conList}" var="con">
        Name:{!con.Name}<br/>
                                    Phone:{!con.Phone}<br/>
        Email:{!con.Email}<br/><hr/>
    </aura:iteration>
</aura:component>